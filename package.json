{
  "name": "llmasistance",
  "displayName": "Llmasistance",
  "version": "0.0.1",
  "description": "extention for llm asistance",
  "author": "tem.nission@gmail.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "build:tailwind": "tailwindcss -i ./styles/globals.css -o ./build/tailwind.css",
    "dev:tailwind": "tailwindcss -i ./styles/globals.css -o ./build/tailwind.css --watch"
  },
  "dependencies": {
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "webRequest",
      "storage",
      "tabs",
      "cookies"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
}
